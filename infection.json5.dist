{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "phpUnit": {
        "configDir": "."
    },
    "mutators": {
        "@default": true,
        "ArrayItem": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\ActivityPub\\WebFinger",
            ]
        },
        "ArrayItemRemoval": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\ActivityPub\\WebFinger",
            ]
        },
        "BitwiseOr": {
            "ignoreSourceCodeByRegex": [
                "JSON_"
            ]
        },
        "CastInt": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58VarTime"
            ]
        },
        "Coalesce": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\ActivityPub\\WebFinger"
            ]
        },
        "Concat": false,
        "ConcatOperandRemoval": false,
        "DecrementInteger": {
            "ignore": [
                "FediE2EE\\PKD\\PublicKey::encodePem",
                "FediE2EE\\PKD\\UtilTrait::stripNewlines",
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "FediE2EE\\PKD\\PublicKey::encodePem",
                "FediE2EE\\PKD\\UtilTrait::stripNewlines",
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ],
            "ignoreSourceCodeByRegex": [
                "hash_hkdf",
                "substr"
            ]
        },
        "LogicalOr": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "Minus": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "Plus": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "RoundingFamily": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "ShiftRight": {
            "ignore": [
                "FediE2EE\\PKD\\Crypto\\Encoding\\Base58BtcVarTime"
            ]
        },
        "Throw_": false
    },
    "threads": "max",
    "timeout": 300,
    "minCoveredMsi": 86,
    "logs": {
        "text": "infection.log"
    }
}